<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.evento">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/evento_logo"
        android:label="@string/app_name"
        android:screenOrientation="portrait"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MenuActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Results"
            android:label="Results"
            android:screenOrientation="portrait"
            android:theme="@style/Results">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.evento.MenuActivity" />
        </activity>
        <activity
            android:name=".Notifications"
            android:label="Notifications"
            android:screenOrientation="portrait"
            android:theme="@style/Notifications">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.evento.MenuActivity" />
        </activity>
        <activity
            android:name=".Schedule"
            android:label="Schedule"
            android:screenOrientation="portrait"
            android:theme="@style/Schedule">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.evento.MenuActivity" />
        </activity>
        <activity
            android:name=".Gallery"
            android:label="Photo Gallery"
            android:screenOrientation="portrait"
            android:theme="@style/Galery">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.evento.MenuActivity" />
        </activity>
        <activity
            android:name=".Dedications"
            android:label="Dedications"
            android:screenOrientation="portrait"
            android:theme="@style/Dedications">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.evento.MenuActivity" />
        </activity>
        <activity
            android:name=".About"
            android:label="About Us"
            android:screenOrientation="portrait"
            android:theme="@style/About">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.evento.MenuActivity" />
        </activity>
        <activity
            android:name=".ViewImage"
            android:label="Photo Gallery"
            android:theme="@style/Galery" />
        <activity
            android:name=".Rules"
            android:label="Rules"
            android:screenOrientation="portrait"
            android:theme="@style/Rules">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.evento.MenuActivity" />
        </activity>
        <activity
            android:name=".AddPhoto"
            android:label="Add Photo"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.CustomOne" />
        <activity
            android:name=".Feedback"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.CustomTheme"></activity>
        <activity
            android:name=".Event"
            android:label="Evento!"
            android:screenOrientation="portrait"
            android:theme="@style/Events">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.evento.MenuActivity" />
        </activity>
    </application>

</manifest>
